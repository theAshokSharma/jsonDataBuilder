{
  "$defs": {
    "Biometric": {
      "properties": {
        "mbr_height_feet": {
          "title": "Height in Feet",
          "type": "integer"
        },
        "mbr_height_inches": {
          "title": "Height Inches",
          "type": "integer"
        },
        "mbr_weight": {
          "title": "Weight",
          "type": "integer"
        },
        "mbr_weight_uom": {
          "title": "Weight (Uom)",
          "type": "string"
        }
      },
      "required": [
        "mbr_height_feet",
        "mbr_height_inches",
        "mbr_weight",
        "mbr_weight_uom"
      ],
      "title": "Biometric",
      "type": "object"
    },
    "CarcinogenExposure": {
      "properties": {
        "mbr_occupation": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Select all professions or Industries, you have worked in"
        },
        "mbr_expose_chem": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Select all hazardous substances/chemicals you are exposed to"
        },
        "mbr_expose_env": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Select all environmental hazards you are exposed to"
        }
      },
      "required": [
        "mbr_occupation",
        "mbr_expose_chem",
        "mbr_expose_env"
      ],
      "title": "CarcinogenExposure",
      "type": "object"
    },
    "Demographic": {
      "properties": {
        "mbr_first_name": {
          "title": "First Name",
          "type": "string"
        },
        "mbr_last_name": {
          "title": "Last Name",
          "type": "string"
        },
        "mbr_date_of_birth": {
          "format": "date",
          "title": "Date Of Birth (mm/dd/yyyy)",
          "type": "string"
        },
        "mbr_gender_at_birth": {
          "title": "Your Gender At Birth",
          "type": "string"
        },
        "mbr_race": {
          "title": "Race",
          "type": "string"
        },
        "mbr_ethnicity": {
          "title": "Ethnicity",
          "type": "string"
        },
        "mbr_address": {
          "$ref": "#/$defs/MemberAddress"
        },
        "mbr_country_lived_in": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Member Country Lived In"
        }
      },
      "required": [
        "mbr_first_name",
        "mbr_last_name",
        "mbr_date_of_birth",
        "mbr_gender_at_birth",
        "mbr_race",
        "mbr_ethnicity",
        "mbr_address",
        "mbr_country_lived_in"
      ],
      "title": "Demographic",
      "type": "object"
    },
    "Genetics": {
      "properties": {
        "mbr_genetic_test_que": {
          "title": "Have you ever had genetic testing done for cancer risk?",
          "type": "string"
        },
        "mbr_genetic_mutation": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Select all genetic mutation you are informed aabout"
        },
        "mbr_genetic_syndrome": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "title": "Select all the genetic syndromes your healthcare provider informed you about"
        }
      },
      "required": [
        "mbr_genetic_test_que",
        "mbr_genetic_mutation",
        "mbr_genetic_syndrome"
      ],
      "title": "Genetics",
      "type": "object"
    },
    "LifeStyle": {
      "properties": {
        "mbr_smoke_cigarette": {
          "title": "Do you smoke cigarettes?",
          "type": "string"
        },
        "mbr_cig_a_day": {
          "title": "Approximate number of cigarettes you smoke/smoked a day?",
          "type": "integer"
        },
        "mbr_cig_total_yrs": {
          "title": "For how many total years did you smoke (even if there were breaks)?",
          "type": "integer"
        },
        "mbr_cig_start_year": {
          "title": "Year you started smoking",
          "type": "integer"
        },
        "mbr_cig_end_year": {
          "title": "Year you stopped smoking",
          "type": "integer"
        },
        "mbr_smoke_cigar_pipe": {
          "title": "Do you smoke cigar or use a pipe?",
          "type": "string"
        },
        "mbr_smoke_ecig_vape": {
          "title": "Do you currently use e-cigarettes or vape products?",
          "type": "string"
        },
        "mbr_second_hand_smoking": {
          "title": "Have you been exposed to secondhand smoke by spending significant time around people who smoke (home, workplace, social settings)?",
          "type": "string"
        },
        "mbr_betel_leaf_quid": {
          "title": "Do you regularly consume excessive amounts of Paan (Betel leaf / quid)?",
          "type": "string"
        },
        "mbr_drink_alcohol_que": {
          "title": "How frequently do you consume alcohol ?",
          "type": "string"
        },
        "mbr_drink_avg_consumption": {
          "title": "Average number of drinks?",
          "type": "integer"
        },
        "mbr_fruit_veg_servings": {
          "title": "On average, how many cups of fruits / vegetables do you eat each day? (need to provide definition of cups of fruits/vegetables)",
          "type": "string"
        },
        "mbr_grain_servings": {
          "title": "How many servings of grains do you usually eat per day?",
          "type": "string"
        },
        "mbr_red_meat_servings": {
          "title": "How often do you eat red meat?",
          "type": "string"
        },
        "mbr_processed_meat_servings": {
          "title": "How often do you eat processed meat",
          "type": "string"
        },
        "mbr_ultra_processed_food_servings": {
          "title": "How often do you consume ultra-processed foods (e.g., packaged snacks, instant noodles, frozen meals)?",
          "type": "string"
        },
        "mbr_sugary_drink": {
          "title": "How often do you drink sugary beverages (e.g., soda, sweetened teas, energy drinks)?",
          "type": "string"
        },
        "mbr_high_sps_foods": {
          "title": "Do you eat a diet high in salty, pickled, or smoked foods?",
          "type": "string"
        },
        "mbr_mod_intense_hrs": {
          "title": "Physical Activity - Moderate-Intensity Activities - Estimated Hours/Week",
          "type": "integer"
        },
        "mbr_mod_intense_mins": {
          "title": "Mod Intense Minutes",
          "type": "integer"
        },
        "mbr_vigorous_hrs": {
          "title": "Physical Activity - Vigorous-Intensity Activities - Estimated Hours/Week",
          "type": "integer"
        },
        "mbr_vigorous_mins": {
          "title": "Member Vigorous Minutes",
          "type": "integer"
        },
        "mbr_birth_control_use": {
          "title": "Have you ever used following Birth Control methods such as (Oral contraceptives, Birth control shots, Birth control implant, IUD, skin patches, vaginal rings)?",
          "type": "string"
        }
      },
      "required": [
        "mbr_smoke_cigarette",
        "mbr_cig_a_day",
        "mbr_cig_total_yrs",
        "mbr_smoke_cigar_pipe",
        "mbr_smoke_ecig_vape",
        "mbr_second_hand_smoking",
        "mbr_betel_leaf_quid",
        "mbr_drink_alcohol_que",
        "mbr_drink_avg_consumption",
        "mbr_fruit_veg_servings",
        "mbr_grain_servings",
        "mbr_red_meat_servings",
        "mbr_processed_meat_servings",
        "mbr_ultra_processed_food_servings",
        "mbr_sugary_drink",
        "mbr_high_sps_foods",
        "mbr_mod_intense_hrs",
        "mbr_mod_intense_mins",
        "mbr_vigorous_hrs",
        "mbr_vigorous_mins",
        "mbr_birth_control_use"
      ],
      "title": "LifeStyle",
      "type": "object"
    },
    "MedicalHistory": {
      "properties": {
        "mbr_diag_cancer_que": {
          "title": "Have you ever been diagnosed with cancer?",
          "type": "string"
        },
        "mbr_cancer_types_hist": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Select type of cancer were you diagnosed with"
        },
        "mbr_chronic_medical_que": {
          "title": "Do you have any chronic medical condition?",
          "type": "string"
        },
        "mbr_chronic_medical_conditions": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Select chronic conditions you currently have or diagnosed with"
        },
        "mbr_infection_diagnosis": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Select Infection Disease you ever been diagnosed with"
        },
        "mbr_general_diagnosis": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Select all the condition you were diagnosed with"
        },
        "mbr_rcvd_radiation_therapy": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Select all areas Radiation Therapy you have received"
        },
        "mbr_undescend_testes": {
          "title": "Do you have undescended testes?",
          "type": "string"
        },
        "mbr_CIS_testes": {
          "title": "Do you have Carcinoma in Situ (CIS) of testes?",
          "type": "string"
        },
        "mbr_menstruating_age_12_55": {
          "title": "Did you start menstruating before age 12 or experience menopause after age 55 years?",
          "type": "string"
        },
        "mbr_ever_pregnant": {
          "title": "Have you ever been pregnant?",
          "type": "string"
        },
        "mbr_ever_full_term_preg": {
          "title": "Have you ever had a full-term pregnancy?",
          "type": "string"
        },
        "mbr_first_full_term_preg": {
          "title": "Was your first full term pregnancy after the age of 35 years?",
          "type": "string"
        },
        "mbr_menopause_status": {
          "title": "Have you reached menopause?",
          "type": "string"
        },
        "mbr_hormone_therapy_aft_menopause": {
          "title": "Have you used hormone replacement therapy after menopause?",
          "type": "string"
        },
        "mbr_ivf_status": {
          "title": "Have you undergone in vitro fertilization (IVF)?",
          "type": "string"
        },
        "mbr_blood_type": {
          "title": "Select your Blood Type",
          "type": "string"
        },
        "mbr_dysphagia_status": {
          "title": "Do you have difficulty swallowing (dysphagia)?",
          "type": "string"
        },
        "mbr_stomach_surgery_status": {
          "title": "Have you undergone stomach surgery for ulcers or other conditions?",
          "type": "string"
        },
        "mbr_vaccines": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Select all the Vaccines you have received"
        },
        "mbr_family_cancer_hist": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {},
              "type": "array"
            }
          ],
          "title": "Do you have any close family members (such as parents, siblings, or children) who have ever been diagnosed with cancer?"
        },
        "mbr_breast_density_status": {
          "title": "Do you recall the specific term used to describe your breast tissue on your last mammogram report? It often falls into one of these categories:",
          "type": "string"
        },
        "mbr_breast_biospy_diagnosis": {
          "title": "Have you ever had breast biopsy for benign (non-cancerous) condion? If so what was the specific diagnosis.",
          "type": "string"
        },
        "mbr_first_child_before_age_30": {
          "title": "Did you had your first child before age 30",
          "type": "string"
        }
      },
      "required": [
        "mbr_diag_cancer_que",
        "mbr_cancer_types_hist",
        "mbr_chronic_medical_que",
        "mbr_chronic_medical_conditions",
        "mbr_infection_diagnosis",
        "mbr_general_diagnosis",
        "mbr_rcvd_radiation_therapy",
        "mbr_undescend_testes",
        "mbr_CIS_testes",
        "mbr_menstruating_age_12_55",
        "mbr_ever_pregnant",
        "mbr_ever_full_term_preg",
        "mbr_first_full_term_preg",
        "mbr_menopause_status",
        "mbr_hormone_therapy_aft_menopause",
        "mbr_ivf_status",
        "mbr_blood_type",
        "mbr_dysphagia_status",
        "mbr_stomach_surgery_status",
        "mbr_vaccines",
        "mbr_family_cancer_hist",
        "mbr_breast_density_status",
        "mbr_breast_biospy_diagnosis",
        "mbr_first_child_before_age_30"
      ],
      "title": "MedicalHistory",
      "type": "object"
    },
    "MemberAddress": {
      "properties": {
        "mbr_street": {
          "title": "Member Street",
          "type": "string"
        },
        "mbr_city": {
          "title": "Member City",
          "type": "string"
        },
        "mbr_state": {
          "title": "Member State",
          "type": "string"
        },
        "mbr_postal_code": {
          "title": "Member Postal Code",
          "type": "string"
        },
        "mbr_country": {
          "title": "Member Country",
          "type": "string"
        }
      },
      "required": [
        "mbr_street",
        "mbr_city",
        "mbr_state",
        "mbr_postal_code",
        "mbr_country"
      ],
      "title": "Address",
      "type": "object"
    }
  },
  "description": "ResponseRawModel maps to member's response to assessment questionnaire.",
  "properties": {
    "demographic": {
      "$ref": "#/$defs/Demographic"
    },
    "biometric": {
      "$ref": "#/$defs/Biometric"
    },
    "lifestyle": {
      "$ref": "#/$defs/LifeStyle"
    },
    "medical_history": {
      "$ref": "#/$defs/MedicalHistory"
    },
    "carcinogen_exposure": {
      "$ref": "#/$defs/CarcinogenExposure"
    },
    "genetics": {
      "$ref": "#/$defs/Genetics"
    }
  },
  "required": [
    "demographic",
    "biometric",
    "lifestyle",
    "medical_history",
    "carcinogen_exposure",
    "genetics"
  ],
  "title": "ResponseRawModel",
  "type": "object"
}